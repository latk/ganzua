{
  "$defs": {
    "DiffEntry": {
      "properties": {
        "old": {
          "anyOf": [
            {
              "$ref": "#/$defs/LockedPackage"
            },
            {
              "type": "null"
            }
          ]
        },
        "new": {
          "anyOf": [
            {
              "$ref": "#/$defs/LockedPackage"
            },
            {
              "type": "null"
            }
          ]
        },
        "is_major_change": {
          "default": false,
          "description": "True if there was a major version change.\n\nThis doesn't literally mean \"the SemVer-major version component changed\",\nbut is intended to highlight version changes that are likely to have breakage.",
          "title": "Is Major Change",
          "type": "boolean"
        },
        "is_downgrade": {
          "default": false,
          "description": "True if the version was downgraded.",
          "title": "Is Downgrade",
          "type": "boolean"
        },
        "is_source_change": {
          "default": false,
          "description": "True if the package source changed.",
          "title": "Is Source Change",
          "type": "boolean"
        }
      },
      "required": [
        "old",
        "new"
      ],
      "title": "DiffEntry",
      "type": "object"
    },
    "DiffStat": {
      "properties": {
        "total": {
          "title": "Total",
          "type": "integer"
        },
        "added": {
          "title": "Added",
          "type": "integer"
        },
        "removed": {
          "title": "Removed",
          "type": "integer"
        },
        "updated": {
          "title": "Updated",
          "type": "integer"
        }
      },
      "required": [
        "total",
        "added",
        "removed",
        "updated"
      ],
      "title": "DiffStat",
      "type": "object"
    },
    "LockedPackage": {
      "properties": {
        "version": {
          "title": "Version",
          "type": "string"
        },
        "source": {
          "anyOf": [
            {
              "enum": [
                "pypi",
                "default",
                "other"
              ],
              "type": "string"
            },
            {
              "$ref": "#/$defs/SourceRegistry"
            },
            {
              "$ref": "#/$defs/SourceDirect"
            }
          ],
          "title": "Source"
        }
      },
      "required": [
        "version",
        "source"
      ],
      "title": "LockedPackage",
      "type": "object"
    },
    "SourceDirect": {
      "description": "The package is sourced from a specific URL or path, e.g. a Git repo or workspace path.",
      "properties": {
        "direct": {
          "description": "URL or path to the package (directory or archive).",
          "title": "Direct",
          "type": "string"
        },
        "subdirectory": {
          "description": "Only allowed if the source points to an archive file.",
          "title": "Subdirectory",
          "type": "string"
        }
      },
      "required": [
        "direct"
      ],
      "title": "SourceDirect",
      "type": "object"
    },
    "SourceRegistry": {
      "description": "The package is sourced from a third party registry.",
      "properties": {
        "registry": {
          "description": "URL or path to the registry.",
          "title": "Registry",
          "type": "string"
        }
      },
      "required": [
        "registry"
      ],
      "title": "SourceRegistry",
      "type": "object"
    }
  },
  "properties": {
    "stat": {
      "$ref": "#/$defs/DiffStat"
    },
    "packages": {
      "additionalProperties": {
        "$ref": "#/$defs/DiffEntry"
      },
      "title": "Packages",
      "type": "object"
    }
  },
  "required": [
    "stat",
    "packages"
  ],
  "title": "Diff",
  "type": "object"
}